<%- include('partials/header')  %>


<div class="main">
    
    <!-- Sign up form -->
    <section class="signup">
        <div class="container1">
            <div class="signup-content">
                <div class="signup-form">
                    <h2 class="form-title">MOM Submission page</h2>
                    <form class="register-form"  method="post" action="/submitMOM" >
                        <div class="form-group" >

                            <label for="sector" >Sector</label>
                            <input type="text" name="sector"  placeholder="sector"style="width:80%;border-left: 180px solid white;padding:6px 0px;" required />

                        </div>
                        <div class="form-group" >

                            <label for="name" >Name</label>
                            <input type="text" name="name"  placeholder="Name"style="width:80%;border-left: 180px solid white;padding:6px 0px;" required />

                        </div>
                        <div class="form-group" >

                            <label for="email" >Email</label>
                            <input type="text" name="email"  placeholder="email"style="width:80%;border-left: 180px solid white;padding:6px 0px;" required />

                        </div>
                        <div class="form-group">
                            <label for="state">State</label>
                            <input type="text" name="state"  placeholder="state"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <br>
                        <div class="form-group">
                            <label for="objective">Objective</label>
                        </div>
                        <div class="form-group">
                            <textarea name="objective" placeholder="objective" rows="4" cols="50"  required></textarea>
                        </div>
                        <div class="form-group">

                            <label for="liability">liability</label>
                            <input type="number" name="liability"  placeholder="liability"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />    
                        </div>
                        <div class="form-group">
                            <label for="capital">Capital</label>
                            <input type="number" name="capital"  placeholder="capital" style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        <input class="form-submit" type="submit" value="submit">
                        
                        
                    </form>
                </div>
                <div class="signup-image">
                    <figure><img src="logFiles/images/signup-image.jpg" alt="sing up image"></figure>
                    
                </div>
            </div>
        </div>
    </section>


</div>







<!-- Font Icon -->
<link rel="stylesheet" href="logFiles/fonts/material-icon/css/material-design-iconic-font.min.css">
<link rel="stylesheet" href="logFiles/css/style1.css">
<!-- Main css -->
<link rel="stylesheet" href="logFiles/css/style.css">
<script src="logFiles/vendor/jquery/jquery.min.js"></script>
<script src="logFiles/js/main.js"></script>
