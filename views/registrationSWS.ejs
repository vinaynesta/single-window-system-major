<%- include('partials/header')  %>


<div class="main">
    
    <!-- Sign up form -->
    <section class="signup">
        <div class="container1">
            <div class="signup-content">
                <div class="signup-form">
                    <h2 class="form-title">New SWS Registration</h2>
                    <form class="register-form" id="register-formSWS"  method="post" action="/addname"> >
                        <div class="form-group" >

                            <label for="name" >Name</label>
                            <input type="text" name="userNameSWS"  placeholder="Name"style="width:80%;border-left: 180px solid white;padding:6px 0px;" required />

                        </div>
                        <div class="form-group">
                            <label for="name">Age</label>
                            <input type="text" name="age"  placeholder="Age"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <div class="form-group">
                            <label for="name">Mobile Number</label>
                            <input type="text" name="mobileNumber"  placeholder="Mobile Number"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <div class="form-group">

                            <label for="name">Fav color</label>
                            <input type="text" name="favColor"  placeholder="Fav Color"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />    
                        </div>
                        <div class="form-group">
                            <label for="name">Occupancy</label>
                            <input type="text" name="occupancy"  placeholder="Occupancy" style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        
                    </form>
                    <button class="form-submit" id="registrationSWS-button" >Submit</button>
                </div>
                <div class="signup-image">
                    <figure><img src="logFiles/images/signup-image.jpg" alt="sing up image"></figure>
                    
                </div>
            </div>
        </div>
    </section>


</div>







<!-- Font Icon -->
<link rel="stylesheet" href="logFiles/fonts/material-icon/css/material-design-iconic-font.min.css">
<link rel="stylesheet" href="logFiles/css/style1.css">
<!-- Main css -->
<link rel="stylesheet" href="logFiles/css/style.css">
