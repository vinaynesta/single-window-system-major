<%- include('partials/header')  %>


<div class="main">
    
    <!-- Sign up form -->
    <section class="signup">
        <div class="container1">
            <div class="signup-content">
                <div class="signup-form">
                    <h2 class="form-title">New Registration</h2>
                    <form class="register-form"  method="post" action="/registrationn" >
                        <div class="form-group" >

                            <label for="name" >Zone Id</label>
                            <input type="text" name="zoneId"  placeholder="Zone Id"style="width:80%;border-left: 180px solid white;padding:6px 0px;" required />

                        </div>
                        <div class="form-group">
                            <label for="name">Building Classification</label>
                            <input type="text" name="buildingClassification"  placeholder="Build Classification"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <div class="form-group">
                            <label for="name">Floor Number</label>
                            <input type="text" name="floorNo"  placeholder="Floor Number"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <div class="form-group">

                            <label for="name">Base Price</label>
                            <input type="text" name="baseValue"  placeholder="Base Value"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />

                            
                        </div>
                        <div class="form-group">
                            <label for="name">Occupancy</label>
                            <input type="text" name="occupancy"  placeholder="Occupancy" style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        <div class="form-group">
                            <label for="name">Area</label>
                            <input type="text" name="area"  placeholder="Area" style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        <div class="form-group">
                            <label for="name">Construction Date</label>
                            <input type="date" name="constructionDate"   style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        <div class="form-group">
                            <label for="name">Nature Of Usage</label>
                            <input type="text" name="natureOfUsage"  placeholder="Nature of Usage" style="width:70%;border-left: 180px solid white;padding:6px 0px;"required />
                        </div>
                        
                        <div class="form-group">
                            <label for="name">Aadhar Number</label>
                            <input type="text" name="aadharId"  placeholder="Aadhar Number"style="width:70%;border-left: 180px solid white;padding:6px 0px;" required />
                        </div>
                        <input class="form-submit" type="submit" value="submit">
                        
                        
                    </form>
                </div>
                <div class="signup-image">
                    <figure><img src="logFiles/images/signup-image.jpg" alt="sing up image"></figure>
                    
                </div>
            </div>
        </div>
    </section>


</div>







<!-- Font Icon -->
<link rel="stylesheet" href="logFiles/fonts/material-icon/css/material-design-iconic-font.min.css">
<link rel="stylesheet" href="logFiles/css/style1.css">
<!-- Main css -->
<link rel="stylesheet" href="logFiles/css/style.css">
<script src="logFiles/vendor/jquery/jquery.min.js"></script>
<script src="logFiles/js/main.js"></script>
